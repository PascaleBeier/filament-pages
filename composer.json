{
    "name": "ahmedababakr/filament-pages",
    "description": "Highly opinionated Pages for filament",
    "keywords": [
        "filament",
        "laravel",
        "filament-pages"
    ],
    "homepage": "https://github.com/AhmedAbabakr/filament-pages",
    "support": {
        "issues": "https://github.com/AhmedAbabakr/filament-pages/issues",
        "source": "https://github.com/AhmedAbabakr/filament-pages"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pascale Beier",
            "email": "mail@pascalebeier.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.13.5",
        "illuminate/contracts": "^9.0 || ^10.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "pestphp/pest-plugin-livewire": "^1.0",
        "pestphp/pest-plugin-parallel": "^0.3",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Beier\\FilamentPages\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Beier\\FilamentPages\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test:pest": "vendor/bin/pest --parallel --no-coverage",
        "test": [
            "@test:pest",
            "@test:phpstan"
        ],
        "fix": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Beier\\FilamentPages\\FilamentPagesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
